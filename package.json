{
  "name": "soundmap-web",
  "version": "1.0.0",
  "description": "位置情報と連動した音声SNSアプリ",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy",
    "typecheck": "tsc --noEmit",
    "cf-typegen": "wrangler types",
    "db:migrate": "wrangler d1 migrations apply DATABASE",
    "db:migrate:local": "wrangler d1 migrations apply DATABASE --local"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.120.3",
    "@tanstack/react-start": "^1.120.3",
    "bcryptjs": "^2.4.3",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.0",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.7.2",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^4.54.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
