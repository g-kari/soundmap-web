{
  "name": "soundmap-web",
  "version": "1.0.0",
  "description": "位置情報と連動した音声SNSアプリ",
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "wrangler pages dev ./build/client --compatibility-date=2024-01-01 --binding DATABASE --d1 DATABASE",
    "preview": "wrangler pages dev",
    "deploy": "wrangler pages deploy ./build/client",
    "typecheck": "tsc",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "cf-typegen": "wrangler types",
    "db:migrate": "wrangler d1 migrations apply DATABASE",
    "db:migrate:local": "wrangler d1 migrations apply DATABASE --local"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.5.0",
    "@remix-run/cloudflare-pages": "^2.5.0",
    "@remix-run/react": "^2.5.0",
    "bcryptjs": "^2.4.3",
    "isbot": "^4.1.0",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@remix-run/dev": "^2.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
